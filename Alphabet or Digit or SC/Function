#include <stdio.h>
int ads (char c)
{
    if ((c>='a' && c<='z') || (c>='A' && c<='Z'))
        return 1;
    else if (c>='0' && c<='9')
        return 2;
    else
        return 0;
}
int main()
{
    char ch;
    printf ("Enter any character: ");
    scanf ("%c",&ch);
    if (ads(ch)==1)
        printf ("Character '%c' is an Alphabet\n",ch);
    else if (ads(ch)==2)
        printf ("Character '%c' is a Digit\n",ch);
    else
        printf ("Character '%c' is a Special Character\n",ch);
    return 0;
}


