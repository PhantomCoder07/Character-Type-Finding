#include <stdio.h>
int main()
{
    char ch;
    printf ("Enter any character: ");
    scanf ("%c",&ch);
    switch ((ch>='a' && ch<='z') || (ch>='A' && ch<='Z'))
    {
    case 1:
        printf ("Character '%c' is an Alphabet\n",ch);
        break;
    default:
        switch (ch>='0' && ch<='9')
        {
        case 1:
            printf ("Character '%c' is a Digit\n",ch);
            break;
        default:
            printf ("Character '%c' is a Special Character\n",ch);
        }
    }
    return 0;
}
