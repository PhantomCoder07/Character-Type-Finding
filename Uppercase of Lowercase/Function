#include <stdio.h>
int ul(char c)
{
    if (c>='A' && c<='Z')
        return 1;
    else if (c>='a' && c<='z')
        return 2;
    else
        return 0;
}
int main()
{
    char ch;
    printf ("Enter an alphabet: ");
    scanf ("%c",&ch);
    if (ul(ch)==1)
        printf ("Alphabet '%c' is in Uppercase\n",ch);
    else if (ul(ch)==2)
        printf ("Alphabet '%c' is in Lowercase\n",ch);
    else
        printf ("%c is not an alphabet\n",ch);
    return 0;
}

