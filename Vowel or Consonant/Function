#include <stdio.h>
int vc (char c)
{
    if (c=='a' || c=='e' || c=='i' || c=='o' || c=='u' ||
        c=='A' || c=='E' || c=='I' || c=='O' || c=='U')
    {
        return 1;
    }
    else
        return 0;
}
int main()
{
    char ch;
    printf ("Enter an alphabet: ");
    scanf ("%c",&ch);
    if (vc(ch))
        printf ("Alphabet '%c' is Vowel\n",ch);
    else
        printf ("Alphabet '%c' is Consonant\n",ch);
    return 0;
}

