#include <stdio.h>
int main()
{
    char ch;
    printf ("Enter an alphabet: ");
    scanf ("%c",&ch);
    switch (ch)
    {
    case 'a':
    case 'e':
    case 'i':
    case 'o':
    case 'u':
    case 'A':
    case 'E':
    case 'I':
    case 'O':
    case 'U':
        printf ("The given alphabet '%c' is Vowel\n",ch);
        break;
    default:
        printf ("The given alphabet '%c' is Consonant\n",ch);
    }
    return 0;
}
